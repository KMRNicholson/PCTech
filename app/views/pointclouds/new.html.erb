<% if !@pointcloud.errors.empty? %>
   <div class = "alert alert-error">
      
      <ul>
         <% @pointcloud.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
         <% end %>
      </ul>
      
   </div>
<% end %>

<h1>Upload File</h1>

<div class = "well">
   <%= form_for @pointcloud, html: { multipart: true } do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
      <%= f.submit "Upload", class: "btn btn-primary" %>
   <% end %>
</div>

